---
vagrantfile-local:
    vm:
        box: puphpet/ubuntu1404-x64
        box_url: puphpet/ubuntu1404-x64
        hostname: '{DOMAIN}'
        memory: '1024'
        cpus: '1'
        chosen_provider: virtualbox
        network:
            private_network: {IP}
            forwarded_port: {  }
        post_up_message: ''
        provider:
            virtualbox:
                modifyvm:
                    natdnshostresolver1: on
            vmware:
                numvcpus: 1
            parallels:
                cpus: 1
        provision:
            puppet:
                manifests_path: puphpet/puppet
                manifest_file: site.pp
                module_path: puphpet/puppet/modules
                options:
                    - '--verbose'
                    - '--hiera_config /vagrant/puphpet/puppet/hiera.yaml'
                    - '--parser future'
        synced_folder:
            SfJy0favbF8q:
                source: ./.modman
                target: /home/vagrant/.modman
                sync_type: rsync
                rsync:
                    args:
                        - '-avzO'
                        - '--delete'
                        - '--chmod=Dug=rwx,o=rx,Fug=rw,o=r'
                    exclude:
                        - .git/
                        - /src
                    auto: 'true'
                    owner: 'www-data'
                    group: 'www-data'
            7OsKjQpGX7UE:
                source: ./vendor
                target: /home/vagrant/vendor
                sync_type: rsync
                rsync:
                    args:
                        - '-avzO'
                        - '--delete'
                        - '--chmod=Dug=rwx,o=rx,Fug=rw,o=r'
                    exclude:
                        - .git/
                    auto: 'true'
                    owner: 'www-data'
                    group: 'www-data'
            C2fAKjNZJRgG:
                source: ./src
                target: /home/vagrant/src
                sync_type: rsync
                rsync:
                    args:
                        - '-avzO'
                        - '--delete'
                        - '--chmod=Dug=rwx,o=rx,Fug=rw,o=r'
                    exclude:
                        - .git/
                    auto: 'true'
                    owner: 'www-data'
                    group: 'www-data'
        usable_port_range:
            start: 10200
            stop: 10500
    ssh:
        host: null
        port: null
        private_key_path: null
        username: vagrant
        guest_port: null
        keep_alive: true
        forward_agent: true
        forward_x11: false
        shell: 'bash -l'
    vagrant:
        host: detect
server:
    install: '1'
    packages:
        - vim
        - libnss-mdns
        - curl
        - git
        - language-pack-de-base
firewall:
    install: '1'
    rules: null
apache:
    install: '1'
    settings:
        user: www-data
        group: www-data
        default_vhost: false
        manage_user: false
        manage_group: false
        sendfile: 0
    modules:
        - rewrite
        - headers
        - proxy
        - deflate
        - ssl
        - info
        - expires
    vhosts:
        IMQf46Yg8sej:
            servername: {DOMAIN}
            serveraliases:
                - www.{DOMAIN}
            docroot: /var/www/{DOMAIN}
            port: '80'
            setenv:
                - 'MAGE_IS_DEVELOPER_MODE 1'
            override:
                - All
            options:
                - Indexes
                - FollowSymLinks
                - MultiViews
            engine: php
            custom_fragment: ''
        KMQf46Yg8sej:
            servername: {DOMAIN}
            serveraliases:
                - www.{DOMAIN}
            docroot: /var/www/{DOMAIN}
            port: '443'
            setenv:
                - 'MAGE_IS_DEVELOPER_MODE 1'
            override:
                - All
            options:
                - Indexes
                - FollowSymLinks
                - MultiViews
            engine: php
            custom_fragment: ''
            ssl: '1'
            ssl_cert: ''
            ssl_key: ''
            ssl_chain: ''
            ssl_certs_dir: ''
    mod_pagespeed: 0
nginx:
    install: '0'
php:
    install: '1'
    version: '55'
    composer: '1'
    composer_home: ''
    modules:
        php:
            - cli
            - intl
            - mcrypt
            - gd
            - curl
            - mysql
            - tidy
        pear: {  }
        pecl:
            - pecl_http
    ini:
        display_errors: On
        error_reporting: '-1'
        session.save_path: /var/lib/php/session
    timezone: Europe/Berlin
    mod_php: 0
hhvm:
    install: '0'
    nightly: 0
    composer: '1'
    composer_home: ''
    settings:
        host: 127.0.0.1
        port: '9000'
    ini:
